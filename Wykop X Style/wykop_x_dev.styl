        content: var(--plusesRemoved);
    }
    section[class] .minusesAdded > article > header > div.right > div > section.rating-box:after
    {
        color: var(--redMinuses)
        content: var(--minusesAdded);
    }
    section[class] .minusesRemoved > article > header > div.right > div > section.rating-box:after
    {
        color: var(--numberZero)
        content: var(--minusesRemoved);
    }
    
    // VOTES PER MINUTE
    section.entry
    {
        --ratingBoxWidth: 100px;
    }
    section.rating-box
    {
        flex-wrap: wrap;
        justify-content: end;
        width: max-content;
        width: var(--ratingBoxWidth);
        min-width: var(--ratingBoxWidth);
        row-gap: 4rem;
    }
    
    // plusowanie i minusowanie pod znaleziskami
    section.entry:has(button.minus)
    {
        --ratingBoxWidth: 165px;
    }
    
    
    if(checkEntryPlusesEnable == 1) // xx
    {
        section.rating-box > ul > li.plus
        {
            font-size: 14rem;
            align-items: center;
            display: flex;
            transition: none!important;
        }
        section[data-wxs_votes_up] section.rating-box > ul > li.plus
        {
            font-size: 0rem;
        }
        section.rating-box > ul > li.plus:before
        {
            font-size: 14rem;
            font-weight: 600;
        }
        
        section.rating-box > ul > li.plus:before
        {
            content: var(--votesUp);
        }
        
        
        if(checkEntryPlusesForVotingGame == 1)
        {
            section.entry[data-wxs_voting_game]:not(.reply) > article > header > div.right > div > section.rating-box > ul > li.plus:before
            {
                content: var(--votesUpHidden);
            }
            section.rating-box:has(div.buttons > button.voted) > ul > li.plus:before
            {
                content: var(--votesUp);
                //counter-reset: votesUp var(--votesUp);
                //content: counter(votesUp);
            }
      
    
            section.entry > section.stream > div.content > section.entry:has(.plus):before
            {
                font-size: 20rem;
                position: fixed;
                top: 50%;
                left: 50%;
                content: attr(data-wxs_votes_up);
                content: var(--votingGameLastDigit);
                z-index: 999!important;
                transition: 3s all linear 0s;

                color: var(--greenPluses);
                scale: 1;
                opacity: 0;
            }
            section.entry > section.stream > div.content > section.entry:has(.plus.voted):before
            {
                color: var(--greenPluses);
                scale: 32;
                opacity: 0.4;
            }
            @starting-style
            {
                section.entry > section.stream > div.content > section.entry:has(.plus):before
                {
                    scale: 1;
                    opacity: 1;
                }
            }
        }


    
    }
    
    section.rating-box .wxs_votes_per_hour
    {
        //display: none;
    }
    section.rating-box .wxs_votes_per_hour[data-wxs_votes_per_hour]
    {
        display: block;
        font-size: 10rem;
        font-weight: bolder;
        color: green;
        text-align: right;
        background-color: rgba(0, 0, 0, 0.2);
        padding: 3px 5px;
        width: 100%;
    }
    
    section.wxs_minimized .wxs_votes_per_hour
    {
        display: none!important;
    }
    
    section.rating-box .wxs_votes_per_hour[data-wxs_votes_per_hour^="+0"],
    section.rating-box .wxs_votes_per_hour[data-wxs_votes_per_hour^="0"]
    {
        display: none;
    }

    section.rating-box .wxs_votes_per_hour[data-wxs_votes_per_hour]::before
    {
        content: attr(data-wxs_votes_per_hour);
    }


    // VOTINGEXPLOSION : animacja plusowania Wykop XS
    section.entry article > header 
    {
        position: relative;
        //z-index: 10;
    }
    section.rating-box
    {
        position: relative;
        z-index: 1; // rating box zindex
    }
    section.rating-box > div.buttons > button.minus
    {
        margin-left: 14px;
    }
    
    if(votingExplosionEnable == 1)
    {
        section.rating-box
        {
            --explosionDelay: 0s;
            --explosionDuration: 900ms;
            --explosionTiming: linear;
        }
        
        .wykopxs_vote_animation
        {
            opacity: 0;
            cursor: pointer;
            font-size: 12rem;
            font-weight: bolder;
            position: absolute;
            top: -10px;
            animation-name: explodePluses;
            animation-delay: var(--explosionDelay);
            animation-duration: var(--explosionDuration);
            animation-timing-function: var(--explosionTiming);
        }
        
        
        /* plusy */
        .wykopxs_vote_animation.wykopxs_green
        {
            color: var(--green)!important;
        /*  text-shadow: 0px 0px 2px var(--green); */
        }

        .wykopxs_vote_animation.wykopxs_golden
        {
            font-size: 35rem;
            color: var(--golden)!important;
            text-shadow: 0px 0px 5px var(--golden);
        } 

        /* minusy */
        .wykopxs_vote_animation.wykopxs_red
        {
            color: var(--red)!important;
        }
    

        
        @keyframes explodePluses
        {
            0%      { transform: translate3d(0, 0, 0); opacity: 0; } 
            12%     { opacity: 0.2; } 
            13%     { opacity: 1; } 
            70%     { opacity: 1; } 
            90%     { opacity: 0.4; } 
            100%    { transform: translate3d(var(--position_x), var(--position_y), var(--position_z)); opacity: 0; } 
        } 
        
        if(votingExplosionAnimation == "animacja2")
        {
            .wykopxs_vote_animation 
            {
                animation-name: explodePluses, explodePlusesAnimacja2;
            }
            
            @eyframes explodePlusesAnimacja2
            {
                0%      { scale: 60%;   } 
                12%     { scale: 60%;   } 
                13%     { scale: 60%;   } 
                70%     { scale: 60%;   } 
                90%     { scale: 120%;  } 
                100%    { scale: 400%;  } 
            }
        } 
        else if(votingExplosionAnimation == "animacja3")
        {
            .wykopxs_vote_animation 
            {
                animation-name: explodePluses, explodePlusesAnimacja3;
            }
            @keyframes explodePlusesAnimacja3
            {
                0%      { scale: 60%;   } 
                12%     { scale: 60%;   } 
                13%     { scale: 60%;   } 
                70%     { scale: 60%;   } 
                90%     { scale: 120%;  } 
                100%    { scale: 400%;  } 
            }
        }
    }
    

    
    
    
    

        
    
    
