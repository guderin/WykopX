        --niebieskiPasek04:                                   rgba(70, 171, 242, 0.4);  
        --niebieskiPasek03:                                   rgba(70, 171, 242, 0.3);  
        --niebieskiPasek02:                                   rgba(70, 171, 242, 0.2);  
        --niebieskiPasek01:                                   rgba(70, 171, 242, 0.1);  
        --niebieskiPasek005:                                  rgba(70, 171, 242, 0.05);  
        --niebieskiPasek002:                                  rgba(70, 171, 242, 0.02);  
