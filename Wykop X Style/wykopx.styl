@-moz-document url-prefix("https://wykop.pl"), url-prefix("http://wykopx.pl")
{    
    version = "2.36";
    i = !important;
    
   
    /* Wykop XS Settings  */
    body { --version:   version;    }    
    body { --homepagePinnedEntriesHideBelowLimit:   homepagePinnedEntriesHideBelowLimit;    }    
    body { --mouseClickOpensImageInNewTab:          mouseClickOpensImageInNewTab;           }
    
    
    

    
    if(votingExplosion == 1)                                { body { --votingExplosion: "true"; }}                                                         
    if(tagHeaderEditable == 1)                              { body { --tagHeaderEditable: "true"; }}
    if(linksAnalyzerEnable == 1)                            { body { --linksAnalyzerEnable: "true"; }}                                                         
    if(linksAnalyzerSortByVotesCount == 1)                  { body { --linksAnalyzerSortByVotesCount: "true"; }}                                                         
    if(allowToDownloadImage == 1)                           { body { --allowToDownloadImage: "true"; }}
    if(autoOpenMoreContentEverywhere == 1)                  { body { --autoOpenMoreContentEverywhere: "true"; }}
    if(showObservedTagsAlphabetically == 1)                 { body { --showObservedTagsAlphabetically: "true"; }}
    if(fixCaseSensitiveTagsRedirection == 1)                { body { --fixCaseSensitiveTagsRedirection: "true"; }}
    if(categoryRedirectToMicroblogButton == 1)              { body { --categoryRedirectToMicroblogButtonFilter: categoryRedirectToMicroblogButtonFilter; }}
    if(refreshOrRedirectOnHomeButtonClick == 1)             { body { --refreshOrRedirectOnHomeButtonClick: "true"; }}
    if(showNotificationsNumberInTitleTags == 1)             { body { --showNotificationsNumberInTitleTags: "true"; }}
    if(showNotificationsNumberInTitleEntries == 1)          { body { --showNotificationsNumberInTitleEntries: "true"; }}
    if(middleClickOpensNotificationsInNewTab == 1)          { body { --middleClickOpensNotificationsInNewTab: "true"; }}
    if(showNotificationsNumberInTitleMessages == 1)         { body { --showNotificationsNumberInTitleMessages: "true"; }}
    if(refreshOrRedirectOnMicroblogButtonClick == 1)        { body { --refreshOrRedirectOnMicroblogButtonClick: "true"; }}
    
    if(enableNotatkowator == 1)                             { body { --enableNotatkowator: "true"; 
                                                                     --notatkowatorUpdateInterval: notatkowatorUpdateInterval;
                                                                    }
                                                            }
    if(showObservedTagsInRightSidebar == 1)                 { body { --showObservedTagsInRightSidebar: "true"; 
                                                                     --showObservedTagsInRightSidebarUpdateInterval: showObservedTagsInRightSidebarUpdateInterval; 
                                                                   }
                                                            }



   

    
    
    @keyframes rotate { to {--angle: 360deg; }}

    @property --angle
    {
      syntax: '<angle>';
      initial-value: 0deg;
      inherits: false;
    }

    :root
    {
        /* WykopX special */
        --gradient-border-width: 2px;
        --gradient-animation: 3s rotate linear infinite;
        
        --microblogEntriesSpacing: microblogEntriesSpacing; /* odległość pomiędzy wpisami na mikroblogu */
        --linksSpacing: linksSpacing;   /* odstępy pomiędzy znaleziskami */
    }
   
    * {
      --angle: 0deg; 
      
      if(gradientBorders == "teczowe")
      {
        --gradient-border-width: 1px;
        --gradient-border-image: conic-gradient(from var(--angle), red, yellow, lime, aqua, blue, magenta, red) 1;
      }
      else if(gradientBorders == "specjalne")
      {
        --gradient-border-width: 0px 5px 0px 0px;
        --gradient-border-image: conic-gradient(from var(--angle), rgba(222, 81, 153, 1), rgba(102, 128, 253, 1), rgba(222, 81, 153, 1)) 1;
      }
      else if(gradientBorders == "ciemne")
      {
        --gradient-border-width: 0px 5px 0px 0px;
        --gradient-border-image: conic-gradient(from var(--angle), rgba(0, 0, 0, 1), rgba(30, 30, 30, 1), rgba(90, 90, 90, 1), rgba(0, 0, 0, 1)) 1;
      }
    }
    
    
    
    
    
    :root
    {

        /* ODLEGLOSCI, WIELKOSCI */
        --gapWidth:                                           gapWidth;
      

        /* KOLORY */
